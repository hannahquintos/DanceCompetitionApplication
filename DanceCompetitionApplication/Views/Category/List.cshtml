@model IEnumerable<DanceCompetitionApplication.Models.CategoryDto>

@{
    ViewBag.Title = "List";
}

<h2>Categories</h2>

@foreach (DanceCompetitionApplication.Models.CategoryDto Category in Model)
{
    <div class="itemContainer"><a class="item" href="/Category/Details/@Category.CategoryId">@Category.CategoryName</a></div>
}

<div class="addBtn" id="addCategoryBtn">
    <a href="/Category/New">Add Category +</a>
</div>
